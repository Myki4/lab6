#include <iostream>
#include <cmath>
#include <vector>

template <typename T>
T geometricMean(const std::vector<T>& array)
{
    T product = 1;
    int n = array.size();

    for (const auto& element : array)
    {
        product *= element;
    }

    return pow(product, 1.0 / n);
}

int main()
{
    using namespace std;
    setlocale(LC_ALL, "Ukrainian");
    int y = 5;
    vector<double> u = { 1.2, 2.0, 3.7, 4.0, 5.5 };

    cout << "Масив: ";
    for (int i = 0; i < y; i++)
    {
        cout << u[i] << " ";
    }
    cout << endl;

    double result = geometricMean(u);
    cout << "Середнє геометричне: " << result << std::endl;

    return 0;
}
